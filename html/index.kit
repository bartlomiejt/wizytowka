    <section class="numbers">
        <div class="numbers__shadow"></div>
        <div class="wrapper">

            <div class="numbers__box">
                <div class="numbers__box-item">

                    <div class="counter" data-number="100">0</div>
                    <p>procent włożonego serca</p>
                </div>
